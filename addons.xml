                <label>30563</label>
                <visible>ListItem.Property(RetrospectFolder)+!ListItem.Property(RetrospectCloaked)+!ListItem.Property(RetrospectFavorite)</visible>
            </item>

            <item library="resources\menus\menu_uncloak.py">
                <label>30564</label>
